{
  "name": "ai-project-estimator",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@9.11.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:frontend": "pnpm -C apps/frontend dev",
    "dev:backend": "pnpm -C apps/backend dev",
    "build": "pnpm -C apps/frontend build && pnpm -C apps/backend build",
    "build:frontend": "pnpm -C apps/frontend build",
    "build:backend": "pnpm -C apps/backend build",
    "lint": "pnpm -C apps/frontend lint && pnpm -C apps/backend lint",
    "lint:frontend": "pnpm -C apps/frontend lint",
    "lint:backend": "pnpm -C apps/backend lint"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  }
}